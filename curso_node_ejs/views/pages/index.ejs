<!DOCTYPE html>

<%- include('../partials/head'); %>


  <html lang="pt-br">

  <body class="container">
    <%- include('../partials/header'); %>
      <main>
        <div class="jumbotron">
          <h1>Node com EJS</h1>
          <p>Vamos aprender a trabalhar com arquivos EJS</p>
          <p>
            <%= subtitle %>
          </p>
        </div>

        <ul>
          <!-- ForEach vai de elemento por elemento rodando o q esta dentro -->
          <% qualitys.forEach(function(quality){ %>
            <li>
              <strong>
                <%= quality.title %>
              </strong>
              <%=quality.message %>
            </li>
            <%})%>

        </ul>
      </main>
      <%- include('../partials/footer'); %>
  </body>

  </html>